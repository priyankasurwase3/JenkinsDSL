job('mavenJob'){
description ('This is maven seed job')
steps {
scm{
 git{
  remote{
   url('https://github.com/priyankasurwase3/simple-java-maven-app.git')
    }
branch('master')
   }
}
maven {
goals('-B -DskipTests clean package')
mavenInstallation('Maven')
 }
maven {
goals('test')
mavenInstallation('Maven')
 }
 }
}
